@model dt191gProjectApp.Models.TypeOfWorkout

@{
    ViewData["Title"] = "Edit koncept";
}


<div class="handleforms">
        <form asp-action="EditType">
        <h2 class="adminformsh2">Redigera koncept</h2>
            <input type="hidden" asp-for="TypeId" />
            <div class="form-group">
                <label asp-for="TypeName" class="control-label"></label>
                <input asp-for="TypeName" class="form-control" />
                <span asp-validation-for="TypeName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Description" class="control-label"></label>
                <textarea asp-for="Description"></textarea>
                <span asp-validation-for="Description" class="text-danger"></span>
            </div>
            <div class="submit-group">
                <input type="submit" value="Uppdatera koncept" class="btn btn-primary" />
                <a class="go" asp-controller="Home" asp-action="Index">&lt; Gå tillbaka</a>
            </div>
        </form>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
